  "scripts": {
    "download-translated-files": "node build-helper/download-translated-files.js",
    "prebuild": "npm run download-translated-files && copyfiles src/default_translations/en-us.json public/assets/translations -f",
    "build": "react-scripts build && node scripts/sentry-release-publisher.js && sh ./build-helper/sourceMapCleanup.sh",
    "postbuild": "rm -rf ./docker/build && cp -r build docker/",
    "start": "react-scripts start",
    "start42": "PORT=4200 react-scripts start",
    "test:dev": "majestic",
    "test:prod": "set CI=true && jest --clear-cache && react-scripts test --silent --env=jsdom --coverage --watchAll=false --json --runInBand --logHeapUsage --outputFile=jest-test-results.json --testResultsProcessor jest-sonar-reporter",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "NODE_ENV=development && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "coverageReporters": [
      "html",
      "json",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/src/stories/**",
      "!**/src/pages/**/mocks.js",
      "!**/src/components/DataManager/mocks/*",
      "!**/src/styles/**",
      "!**/src/pages/Bills/TestCaseComponents.js"
    ]
  },
